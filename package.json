{
  "name": "@tkrotoff/bootstrap-floating-label",
  "version": "0.0.1",
  "description": "Bootstrap 4 Floating Label",
  "license": "MIT",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "example/index.html",
  "files": [
    "src/"
  ],
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "yarn run clean && rm -rf node_modules yarn.lock",
    "start": "yarn run clean && parcel example/index.html --open",
    "build": "yarn run clean && parcel build example/index.html --public-url ./",
    "prettier": "prettier --write \"**/*\"",
    "precommit": "yarn prettier && yarn build",
    "prepush": "yarn precommit"
  },
  "dependencies": {},
  "peerDependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "bootstrap": "^4.3.1",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.2",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "typescript": "^3.3.3333"
  }
}
