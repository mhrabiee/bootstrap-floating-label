{
  "name": "bootstrap-floating-label",
  "version": "0.0.1",
  "description": "Bootstrap 4 Floating Label",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "yarn run clean && rm -rf node_modules yarn.lock",
    "start": "yarn run clean && parcel index.html --open",
    "build": "yarn run clean && parcel build index.html --public-url ./",
    "prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2",
    "typescript": "^3.0.3"
  }
}
