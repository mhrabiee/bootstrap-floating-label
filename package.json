{
  "name": "@tkrotoff/bootstrap-floating-label",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/bootstrap-floating-label.git"
  },
  "description": "Bootstrap 4 Floating Label",
  "license": "MIT",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "index.html",
  "files": [
    "src/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "npm run clean && parcel index.html --open",
    "build": "npm run clean && parcel build index.html --public-url ./",
    "build:dev": "npm run clean && NODE_ENV=development parcel build index.html --no-minify --public-url ./",
    "tsc": "tsc",
    "format": "prettier --write \"**/*\"",
    "lint": "eslint . '**/*.{js,jsx,ts,tsx}' && stylelint '**/*.scss'",
    "precommit": "npm run format && npm run tsc && npm run lint && npm run build",
    "prepush": "npm run precommit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "bootstrap": "^4.3.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.8",
    "parcel-bundler": "^1.12.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sass": "^1.23.0",
    "stylelint": "^11.0.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.11.1",
    "typescript": "^3.6.4"
  }
}
