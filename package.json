{
  "name": "bootstrap-floating-label",
  "version": "0.0.1",
  "description": "Bootstrap 4 Floating Label",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "example/index.html",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "yarn run clean && rm -rf node_modules yarn.lock",
    "start": "yarn run clean && parcel example/index.html --open",
    "build": "yarn run clean && parcel build example/index.html --public-url ./",
    "prettier": "prettier --write \"**/*\""
  },
  "dependencies": {},
  "peerDependencies": {
    "bootstrap": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "bootstrap": "^4.2.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "typescript": "^3.2.2"
  }
}
