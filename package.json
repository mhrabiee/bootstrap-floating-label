{
  "name": "bootstrap-floating-label",
  "version": "0.0.1",
  "description": "Bootstrap 4 Floating Label",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "src/test.html",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "yarn run clean && rm -rf node_modules yarn.lock",
    "start": "yarn run clean && parcel src/test.html --open",
    "build": "yarn run clean && parcel build src/test.html --public-url ./",
    "prettier": "prettier --write \"**/*\""
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "autoprefixer": "^9.3.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "typescript": "^3.1.6"
  }
}
