{
  "name": "@tkrotoff/bootstrap-floating-label",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/bootstrap-floating-label.git"
  },
  "description": "Bootstrap 4 Floating Label",
  "license": "MIT",
  "keywords": [
    "bootstrap4",
    "bootstrap",
    "floating",
    "float",
    "label"
  ],
  "main": "example/index.html",
  "files": [
    "src/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist .cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "npm run clean && parcel example/index.html --open",
    "build": "npm run clean && parcel build example/index.html --public-url ./",
    "prettier": "prettier --write \"**/*\"",
    "tsc": "tsc",
    "precommit": "npm run prettier && tsc && npm run build",
    "prepush": "npm run precommit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "bootstrap": "^4.3.1",
    "husky": "^2.4.0",
    "node-sass": "^4.12.0",
    "parcel-bundler": "^1.12.3",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.5.1"
  }
}
